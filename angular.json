{
  "version": 1,
  "projects": {
    "main": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/main",
      "sourceRoot": "apps/main/src",
      "prefix": "wb-umd-test",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/main",
            "index": "apps/main/src/index.html",
            "main": "apps/main/src/main.ts",
            "polyfills": "apps/main/src/polyfills.ts",
            "tsConfig": "apps/main/tsconfig.app.json",
            "aot": false,
            "assets": ["apps/main/src/favicon.ico", "apps/main/src/assets"],
            "styles": ["apps/main/src/styles.scss"],
            "scripts": [
              "node_modules/systemjs/dist/s.js",
              "node_modules/systemjs/dist/extras/named-register.js",
              "node_modules/systemjs/dist/extras/amd.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/main/src/environments/environment.ts",
                  "with": "apps/main/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "main:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "main:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "main:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/main/tsconfig.app.json",
              "apps/main/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/main/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/main/jest.config.js",
            "tsConfig": "apps/main/tsconfig.spec.json",
            "setupFile": "apps/main/src/test-setup.ts"
          }
        }
      }
    },
    "main-e2e": {
      "root": "apps/main-e2e",
      "sourceRoot": "apps/main-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/main-e2e/cypress.json",
            "tsConfig": "apps/main-e2e/tsconfig.e2e.json",
            "devServerTarget": "main:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "main:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/main-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/main-e2e/**"]
          }
        }
      }
    },
    "whiteboard": {
      "projectType": "library",
      "root": "libs/whiteboard",
      "sourceRoot": "libs/whiteboard/src",
      "prefix": "wb-umd-test",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/whiteboard/tsconfig.lib.json",
              "libs/whiteboard/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/whiteboard/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/whiteboard/jest.config.js",
            "tsConfig": "libs/whiteboard/tsconfig.spec.json",
            "setupFile": "libs/whiteboard/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "umd": {
      "projectType": "library",
      "root": "libs/umd",
      "sourceRoot": "libs/umd/src",
      "prefix": "wb-umd-test",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/umd/tsconfig.lib.json",
              "libs/umd/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/umd/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/umd/jest.config.js",
            "tsConfig": "libs/umd/tsconfig.spec.json",
            "setupFile": "libs/umd/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "widgets": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "widgets",
      "sourceRoot": "libs/widgets/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "./builders:template-builder",
          "options": {
            "outputPath": "dist/widgets",
            "index": "",
            "main": "libs/widgets/src/main.ts",
            "polyfills": "libs/widgets/src/polyfills.ts",
            "tsConfig": "libs/widgets/tsconfig.app.json",
            "assets": [],
            "styles": [],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "main"
}
